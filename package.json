		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@types/node-fetch": "^2.6.11",
		"node-fetch": "^3.3.2",
		"obsidian": "^1.1.1"
